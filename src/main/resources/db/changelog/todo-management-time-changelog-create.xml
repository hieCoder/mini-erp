<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.0.xsd">

    <!-- create table TODO_MANAGEMENT_TIME -->
    <changeSet id="1" author="team2">
        <createTable tableName="TODO_MANAGEMENT_TIME">
            <column name="ID" type="VARCHAR(255)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="DAY" type="DATE"/>
            <column name="TARGET" type="VARCHAR(255)"/>
            <column name="PERFORMANCE" type="BIT"/>
            <column name="USER_ID" type="VARCHAR(255)"/>
            <column name="CATEGORY_MANAGEMENT_TIME_ID" type="VARCHAR(255)"/>
            <column name="MANAGEMENT_TIME_ID" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
