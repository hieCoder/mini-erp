<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.0.xsd">

    <!-- create table YEAR_MANAGEMENT_TIME_DAY -->
    <changeSet id="1" author="team2">
        <createTable tableName="YEAR_MANAGEMENT_TIME_DAY">
            <column name="ID" type="VARCHAR(255)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="CODE" type="VARCHAR(255)"/>
            <column name="TARGET" type="TEXT"/>
            <column name="USER_ID" type="VARCHAR(255)"/>
            <column name="CATEGORY" type="VARCHAR(255)"/>
            <column name="GRATEFUL" type="VARCHAR(1500)"/>
            <column name="HAPPY" type="VARCHAR(1500)"/>
            <column name="WHO_U_BECOME" type="VARCHAR(1500)"/>
            <column name="PERSONAL_GOAL" type="VARCHAR(1500)"/>
            <column name="COMMENDABLE" type="VARCHAR(1500)"/>
            <column name="KEYWORDS_3" type="VARCHAR(1500)"/>
            <column name="COLOR" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
